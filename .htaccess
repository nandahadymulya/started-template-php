RewriteEngine On

Options All -Indexes
RewriteRule ^([a-zA-Z0-9]+)/?$ ?page=$1 [NC]
RewriteRule ^riwayat-detail/([0-9]+)/?$ index.php?page=riwayat-detail&id=$1 [NC]
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([0-9]+)/?$ index.php?page=$1&act=$2&id=$3 [NC]
RewriteRule ^admin/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/?$ index.php?page=admin&act=$1&id=$2 [NC]
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z]+)/?$ index.php?page=$1&act=$2 [NC]