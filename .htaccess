RewriteEngine On

Options All -Indexes

RewriteRule ^([a-zA-Z0-9]+)/?$ ?view=$1 [NC]
RewriteRule ^riwayat-detail/([0-9]+)/?$ index.php?view=riwayat-detail&id=$1 [NC]
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([0-9]+)/?$ index.php?view=$1&act=$2&id=$3 [NC]
RewriteRule ^admin/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/?$ index.php?view=admin&act=$1&id=$2 [NC]
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z]+)/?$ index.php?view=$1&act=$2 [NC]